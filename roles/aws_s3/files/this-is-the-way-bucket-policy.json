{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "SensitiveBucketListBucket",
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": ["arn:aws:s3:::SensitiveBucket"]
        },
        {
            "Sid": "SensitiveBucketAllowReadWrite",
            "Effect": "Allow",
            "Action": "s3:*Object",
            "Resource": ["arn:aws:s3:::SensitiveBucket/*"]
        },
        {
            "Sid": "SensitiveBucketAllowReadOnly",
            "Effect": "Allow",
            "Action": [
                "s3:ListAllMyBuckets",
                "s3:ListBucket",
                "s3:GetObject"
            ],
            "Resource": ["arn:aws:s3:::SensitiveBucket/*"]
        }
    ]
}